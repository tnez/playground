{
  "version": 2,
  "name": "playground",
  "builds": [
    {
      "config": { "distDir": ".out" },
      "src": "packages/components/package.json",
      "use": "@now/static-build"
    },
    { "src": "packages/www/package.json", "use": "@now/next" }
  ],
  "routes": [
    { "src": "/storybook", "dest": "/packages/components" },
    { "src": "/(.*)", "dest": "/packages/www/$1" }
  ]
}
