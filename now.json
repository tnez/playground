{
  "version": 2,
  "name": "playground",
  "builds": [
    { "src": "random/*.js", "use": "@now/node" },
    { "src": "www/package.json", "use": "@now/next" },
    { "src": "*.html", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/api/random/(.*)", "dest": "/random/$1" },
    { "src": "/static-page", "dest": "/static-page.html" },
    { "src": "/(.*)", "dest": "/www/$1" }
  ]
}
